{
    "name": "@ylstack-dev/cf-cms-ecommerce",
    "version": "3.0.0",
    "description": "Professional e-commerce plugin for cf-cms.js with multi-store support, products, orders, and payments",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./services": {
            "import": "./dist/services/index.js",
            "types": "./dist/services/index.d.ts"
        },
        "./types": {
            "import": "./dist/types/index.js",
            "types": "./dist/types/index.d.ts"
        }
    },
    "files": [
        "dist",
        "manifest.json",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --dts --outDir dist",
        "dev": "tsup src/index.ts --format esm,cjs --dts --outDir dist --watch",
        "test": "vitest run",
        "test:watch": "vitest",
        "type-check": "tsc --noEmit",
        "lint": "eslint src --ext .ts"
    },
    "dependencies": {
        "@ylstack-dev/cf-cms-core": "^3.0.0",
        "hono": "^4.0.0",
        "zod": "^3.22.0"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20240620.0",
        "@types/node": "^20.0.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.0.0",
        "tsup": "^8.0.0",
        "typescript": "^5.0.0",
        "vitest": "^1.0.0"
    },
    "peerDependencies": {
        "@ylstack-dev/cf-cms-core": "^3.0.0"
    },
    "author": {
        "name": "YLStack Dev",
        "email": "dev@ylstack.com",
        "url": "https://ylstack.com"
    },
    "license": "MIT",
    "keywords": [
        "cf-cms",
        "plugin",
        "ecommerce",
        "store",
        "products",
        "orders",
        "payments",
        "cloudflare",
        "workers"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/ylstack-dev/cf-cms.git",
        "directory": "packages/ecommerce-plugin"
    },
    "homepage": "https://github.com/ylstack-dev/cf-cms/tree/main/packages/ecommerce-plugin",
    "bugs": {
        "url": "https://github.com/ylstack-dev/cf-cms/issues"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}